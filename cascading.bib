@article{Hastings:2010p5336,
author = {Hastings, Alan and Wysham, Derin B},
title = {{Regime shifts in ecological systems can occur with no warning}},
journal = {Ecol Lett},
year = {2010},
volume = {13},
number = {4},
pages = {464--472},
month = apr,
publisher = {Blackwell Publishing Ltd},
affiliation = {Univ Calif Davis, Dept Environm Sci {\&} Policy, Davis, CA 95616 USA},
doi = {10.1111/j.1461-0248.2010.01439.x},
language = {English},
rating = {5},
date-added = {2010-03-10T16:05:25GMT},
date-modified = {2016-09-26T16:32:08GMT},
abstract = {P>Predicting regime shifts - drastic changes in dynamic behaviour - is a key challenge in ecology and other fields. Here we show that the class of ecological systems that will exhibit leading indicators of regime shifts is limited, and that there is a set of ecological models and, therefore, also likely to be a class of natural systems for which there will be no forewarning of a regime change. We first describe how nonlinearities in combination with environmental variability lead to model descriptions that will not have smooth potentials, concluding that many ecological systems are described by systems without smooth potentials and thus will not show typical leading indicators of regime shifts. We then illustrate the impact of these general arguments by numerically examining the dynamics of several model ecological systems under slowly changing conditions. Our results offer a cautionary note about the generality of forecasting sudden changes in ecosystems.},
url = {http://onlinelibrary.wiley.com/doi/10.1111/j.1461-0248.2010.01439.x/full},
local-url = {file://localhost/Users/juanrocha/Dropbox/Library.papers3/Files/AB/ABEDD3FE-D4FF-44D9-87ED-D553C36451DF.pdf},
file = {{ABEDD3FE-D4FF-44D9-87ED-D553C36451DF.pdf:/Users/juanrocha/Dropbox/Library.papers3/Files/AB/ABEDD3FE-D4FF-44D9-87ED-D553C36451DF.pdf:application/pdf;ABEDD3FE-D4FF-44D9-87ED-D553C36451DF.pdf:/Users/juanrocha/Dropbox/Library.papers3/Files/AB/ABEDD3FE-D4FF-44D9-87ED-D553C36451DF.pdf:application/pdf}},
